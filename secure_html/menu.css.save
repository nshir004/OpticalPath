/* Add a black background to the top navigation bar */
.topnav {
	background-color: #333;
	overflow: hidden;
}

/* Style the links inside the nav bar */
.topnav a {
	float: left;
	color: #f2f2f2;
	text-align: center;
	padding: 14px 16px;
	text-decoration: none;
	font-size: 17px;
}

/* Change the color of the links on hover */
.topnav a:hover {
	background-color: #;
	color: black;
}

/* Add color to the active/current link */
.topnav a.active {
	background-color: #04AA6D;
	color: white;
}
